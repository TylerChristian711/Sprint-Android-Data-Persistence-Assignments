CREATE TABLE player (
id INTEGER PRIMARY KEY,  
first_name TEXT,
last_name Text,
age INTEGER,
position TEXT);

INSERT INTO player(id,first_name,last_name,age,position) VALUES(1,"Christian","Wilkins",23,"Defense");

