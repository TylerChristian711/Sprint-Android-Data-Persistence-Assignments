CREATE TABLE team(
id INTEGER PRIMARY KEY,
name TEXT,
superbowls_won INTEGER,
FOREIGN KEY(id) REFERENCES player(id));


INSERT INTO team(id,name,superbowls_won) VALUES (1,"miami dolphins",2);

